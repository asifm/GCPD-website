{
  "name": "patent-viz-site",
  "version": "1.0.0",
  "description": "Global Corporate Patent Data",
  "author": "asif",
  "private": true,
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3000"
    }
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest || exit 0",
    "dev-debug": "node --inspect node_modules/nuxt/bin/nuxt"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@types/crossfilter": "0.0.32",
    "@types/d3-array": "^1.2.4",
    "@types/eslint": "^4.16.5",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/gsap": "^1.20.0",
    "@types/vue-select": "^2.4.0",
    "apexcharts": "^2.4.1",
    "cross-env": "^5.2.0",
    "crossfilter": "^1.3.12",
    "d3": "^5.7.0",
    "d3-array": "^2.0.0",
    "d3-svg-annotation": "^2.3.1",
    "d3-tip": "^0.9.1",
    "element-ui": "^2.4.11",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "glider-js": "^1.6.0",
    "gsap": "^2.0.2",
    "lodash": "^4.17.11",
    "nuxt": "^2.3.4",
    "scrollmagic": "^2.0.6",
    "uikit": "^3.0.0-rc.25",
    "vue-apexcharts": "^1.2.7",
    "vue-scroll": "^2.1.9",
    "vue-select": "^2.5.1",
    "vuetify": "^1.3.11",
    "vuikit": "^0.8.10"
  },
  "devDependencies": {
    "@nuxtjs/markdownit": "^1.2.2",
    "@types/cypress": "^1.1.3",
    "@types/d3": "^5.0.1",
    "@types/jest": "^23.3.10",
    "@types/markdown-it": "0.0.7",
    "@types/node-fetch": "^2.1.4",
    "@types/node-sass": "^3.10.32",
    "@types/prettier": "^1.15.2",
    "@types/pug": "^2.0.4",
    "@types/uikit": "^2.27.6",
    "@vue/test-utils": "^1.0.0-beta.26",
    "babel-eslint": "latest",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "1.7.0",
    "cypress": "^3.1.2",
    "file-loader": "^2.0.0",
    "jest": "^23.6.0",
    "jest-fetch-mock": "latest",
    "jest-vue-preprocessor": "^1.4.0",
    "markdown-it": "^8.4.2",
    "node-fetch": "^2.2.0",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.3",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "vue-jest": "^3.0.1",
    "vue-md-loader": "^1.1.0",
    "wallaby-vue-compiler": "^1.0.3"
